{
	"expo": {
		"name": "Nerdy Sports",
		"slug": "nerdy-sports",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "src/assets/images/icon.png",
		"scheme": "nerdy-sports",
		"userInterfaceStyle": "dark",
		"splash": {
			"backgroundColor": "#0F172A"
		},
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.onyedikachi23.nerdysports"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "src/assets/images/adaptive-icon.png",
				"backgroundColor": "#0F172A"
			},
			"package": "com.onyedikachi23.nerdysports",
			"edgeToEdgeEnabled": true
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "src/assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "src/assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			],
			"expo-font",
			"expo-web-browser"
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
